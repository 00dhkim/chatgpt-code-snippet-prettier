  "manifest_version": 3,
  "name": "Chatgpt Code Snippet Wrapper",
  "short_name": "Snippet Wrap",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*"
      ],
      "js": [
        "index.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}